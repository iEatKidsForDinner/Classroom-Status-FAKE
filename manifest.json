{
  "manifest_version": 3,
  "name": "Classroom Status",
  "version": "1.2.9",
  "description": "Monitors student activity in lessons; the best tool for educators.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["empty.js"],
      "css": ["empty.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
      "icon128.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
